	$("#pop_HUTS1B381B1 .wish_btn_on").css("display", "block");
	var str = "";
	str +=  "<div class='HUTS1B381B1 wish_box'>";
	str +=  "<input type='hidden' name='id_115' value='HUTS1B381B1' id='id_115'>";
	str +=  "<div class='wish_img'>";
	str +=  "<img src='https://rowilab.com/hazzys/images/HUTS1B381B1.jpg'>";
	str +=  "</div>";
	str +=  "<div class='wish_title'>[냉감 긴팔]블루 앞판 스트라이프 긴팔 하이넥 티셔츠<p>126,540</p></div>";
	str +=  "<div class='wish_add'  id='del_HUTS1B381B1'>♡</div></div>";
	document.getElementById('wish_data').innerHTML += str;
});

