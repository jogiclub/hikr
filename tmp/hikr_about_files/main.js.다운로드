function copy() {
  var tempElem = document.createElement('textarea');
  tempElem.value = 'LFQAAANAAFXA1Q';  
  document.body.appendChild(tempElem);

  tempElem.select();
  document.execCommand("copy");
  document.body.removeChild(tempElem);
  alert('쿠폰번호 복사완료하였습니다.\n쇼핑몰이동>MY>나의혜택>쿠폰등록 페이지에서 쿠폰등록 후 사용 가능하십니다.');
}
function copya() {
  var tempElem = document.createElement('textarea');
  tempElem.value = 'LFQHAAFAAFWA1J';  
  document.body.appendChild(tempElem);

  tempElem.select();
  document.execCommand("copy");
  document.body.removeChild(tempElem);
  alert('쿠폰번호 복사완료하였습니다.\n쇼핑몰이동>MY>나의혜택>쿠폰등록 페이지에서 쿠폰등록 후 사용 가능하십니다.');
}


function hideDiv (){
	setTimeout(function() {
	document.getElementById("l01").style.display="none";
	}, 6000);
}
function hideDiv2 (){
	setTimeout(function() {
	document.getElementById("l02").style.display="none";
	}, 6000);
}
function changeIframeUrl(url) {
	document.getElementById("main_frame").src = url;
}



$(document).ready(function(){ 

	$(document).on('click', '.clear', function(){
		$("#wish_data input").remove();
	}); 
	$(document).on('click', '.enter_btn', function(){
		$(".enter_btn").css("display", "none"); 
		$(".swiper-container").css("display", "none"); 
		$(".main_slogan").css("display", "none"); 
	});
	$(document).on('click', '.pop_layer_boxa', function(){
		$("#pop_layer").css("display", "none"); 
		$(".pop_layer_boxa").css("background", "#000"); 
		$(".pop_layer_boxa").css("color", "#fff"); 
		$(".pop_layer_boxb").css("background", "#fff"); 
		$(".pop_layer_boxb").css("color", "#000"); 
		$(".pop_layer_boxc").css("background", "#fff"); 
		$(".pop_layer_boxc").css("color", "#000"); 
		$(".pop_layer_boxd").css("background", "#fff"); 
		$(".pop_layer_boxd").css("color", "#000"); 
		$(".quick_menu_boxb").css("display", "block"); 
		$(".quick_menu_boxb_on").css("display", "none"); 
		$("#mask").css("display", "none"); 
	});
	$(document).on('click', '.pop_layer_boxb', function(){
		$("#pop_layer").css("display", "none"); 
		$(".pop_layer_boxa").css("background", "#fff"); 
		$(".pop_layer_boxa").css("color", "#000"); 
		$(".pop_layer_boxb").css("background", "#000"); 
		$(".pop_layer_boxb").css("color", "#fff"); 
		$(".pop_layer_boxc").css("background", "#fff"); 
		$(".pop_layer_boxc").css("color", "#000"); 
		$(".pop_layer_boxd").css("background", "#fff"); 
		$(".pop_layer_boxd").css("color", "#000"); 
		$(".quick_menu_boxb").css("display", "block"); 
		$(".quick_menu_boxb_on").css("display", "none"); 
		$("#mask").css("display", "none"); 
	});
	$(document).on('click', '.pop_layer_boxc', function(){
		$("#pop_layer").css("display", "none"); 
		$(".pop_layer_boxa").css("background", "#fff"); 
		$(".pop_layer_boxa").css("color", "#000"); 
