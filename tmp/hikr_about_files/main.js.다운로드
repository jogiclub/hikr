	});
	$(document).on('click', '.coupon_close', function(){
		$("#pop_coupon").css("display", "none"); 
		$("#pop_coupona").css("display", "none"); 
		$(".pop_contact").css("display", "none"); 
		$("#mask").css("display", "none"); 
		$("#l01").css("display", "block"); 
		$("#l02").css("display", "block"); 
		$(".quick_menu_boxe").css("display", "block"); 
		$(".quick_menu_boxe_on").css("display", "none"); 
	});
	$(document).on('click', '.databox_close', function(){
		$(".databox").css("display", "none"); 
		$(".quick_menu_boxc").css("display", "block"); 
		$(".quick_menu_boxc_on").css("display", "none"); 
		$("#mask").css("display", "none"); 
	});
	$(document).on('click', '.pop_notice_close', function(){
		$("#pop_notice").css("display", "none"); 
	});

	$(document).on('click', '#audioStop', function(){
		$("#audioPlay").css("display", "inline-block"); 
		$("#audioStop").css("display", "none"); 
	}); 
	$(document).on('click', '#audioPlay', function(){
		$("#audioPlay").css("display", "none"); 
		$("#audioStop").css("display", "inline-block"); 
	});
	$(document).on('click', '.quick_menu_boxb', function(){
		$(".databox").css("display", "none"); 
		$(".pop_contact").css("display", "none"); 
		$(".quick_menu_boxe").css("display", "block"); 
		$(".quick_menu_boxe_on").css("display", "none"); 
		$("#pop_layer").css("display", "block"); 
		$(".quick_menu_boxb").css("display", "none"); 
		$(".quick_menu_boxb_on").css("display", "block"); 
		$(".quick_menu_boxc").css("display", "block"); 
		$(".quick_menu_boxc_on").css("display", "none"); 
		$("#mask").css("display", "block");
		$("#balloonbox").css("display", "none");
	});
	$(document).on('click', '.quick_menu_boxb_on', function(){
		$("#pop_layer").css("display", "none"); 
		$(".quick_menu_boxb").css("display", "block"); 
		$(".quick_menu_boxb_on").css("display", "none"); 
		$("#mask").css("display", "none"); 
		$("#balloonbox").css("display", "none");
	});
	$(document).on('click', '.quick_menu_boxc', function(){
		$(".databox").css("display", "block"); 
		$(".pop_contact").css("display", "none"); 
		$(".quick_menu_boxe").css("display", "block"); 
		$(".quick_menu_boxe_on").css("display", "none"); 
		$("#pop_layer").css("display", "none"); 
		$(".quick_menu_boxb").css("display", "block"); 
		$(".quick_menu_boxb_on").css("display", "none"); 
		$(".quick_menu_boxc").css("display", "none"); 
		$(".quick_menu_boxc_on").css("display", "block"); 
		$("#mask").css("display", "block"); 
		$("#balloonbox").css("display", "none");
	});
	$(document).on('click', '.quick_menu_boxc_on', function(){
		$(".databox").css("display", "none"); 
		$(".quick_menu_boxc").css("display", "block"); 
		$(".quick_menu_boxc_on").css("display", "none"); 
		$("#mask").css("display", "none"); 
	});
	$(document).on('click', '.quick_menu_boxe', function(){
		$(".pop_contact").css("display", "block"); 
		$(".databox").css("display", "none"); 
		$("#pop_layer").css("display", "none"); 
		$(".quick_menu_boxb").css("display", "block"); 
		$(".quick_menu_boxb_on").css("display", "none"); 
		$(".quick_menu_boxc").css("display", "block"); 
		$(".quick_menu_boxc_on").css("display", "none"); 
		$(".quick_menu_boxe").css("display", "none"); 
		$(".quick_menu_boxe_on").css("display", "block"); 
		$("#mask").css("display", "block"); 
		$("#balloonbox").css("display", "none");
	});
	$(document).on('click', '.quick_menu_boxe_on', function(){
		$(".pop_contact").css("display", "none"); 
		$(".databox").css("display", "none"); 
		$("#pop_layer").css("display", "none"); 
		$(".quick_menu_boxb").css("display", "block"); 
		$(".quick_menu_boxb_on").css("display", "none"); 
		$(".quick_menu_boxc").css("display", "block"); 
		$(".quick_menu_boxc_on").css("display", "none"); 
		$(".quick_menu_boxe").css("display", "block"); 
		$(".quick_menu_boxe_on").css("display", "none"); 
		$("#mask").css("display", "none"); 
		$("#balloonbox").css("display", "none");
	});
	$(document).on('click', '.box_g', function(){
		$('#pop_coupon').css('display', 'block');
		$('#mask').css('display', 'block');
		hideDiv();
	});

